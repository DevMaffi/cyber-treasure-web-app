{
    "name": "cyber-treasure-web-app",
    "version": "2024.1-alpha.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build --no-lint",
        "preview": "next start",
        "start": "pnpm db:deploy && next start -p $PORT",
        "lint": "next lint",
        "db:push": "dotenv -e .env.local -- npx prisma db push",
        "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
        "db:studio": "dotenv -e .env.local -- npx prisma studio",
        "db:deploy": "npx prisma migrate deploy && npx prisma generate"
    },
    "dependencies": {
        "@prisma/client": "5.16.1",
        "@tanstack/react-query": "^5.49.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.399.0",
        "next": "14.2.4",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@tanstack/react-query-devtools": "^5.49.2",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "dotenv-cli": "^7.4.2",
        "eslint": "^8",
        "eslint-config-next": "14.2.4",
        "prettier": "3.3.2",
        "prisma": "^5.16.1",
        "sass": "^1.77.6",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    }
}
